# Enable unsafe-perm to prevent permission issues
unsafe-perm=true
# Set the npm registry
registry=https://registry.npmjs.org/
# Disable package-lock.json generation
package-lock=false
# Set the node-linker to node-modules
node-linker=node-modules
# Set the cache directory
cache=.npm-cache
# Set the npm log level
loglevel=warn
# Set the npm audit level
audit-level=low
# Disable package-lock.json
package-lock=false
# Set the npm save exact
save-exact=true
# Set the npm save prefix
save-prefix=""
# Set the npm engine strict
engine-strict=true
# Set the npm prefer-offline
prefer-offline=true
# Set the npm update-notifier
update-notifier=false
# Set the npm progress
progress=false
# Set the npm depth
depth=0
# Set the npm tag-version-prefix
tag-version-prefix=""
# Set the npm git-tag-version
git-tag-version=true
# Set the npm commit-hooks
commit-hooks=true
# Set the npm save-dev
save-dev=true
# Set the npm save-optional
save-optional=true
# Set the npm save-bundle
save-bundle=false
# Set the npm save-peer
save-peer=false
# Set the npm save-prod
save-prod=true
# Set the npm save-prefix
save-prefix=""
# Set the npm save-exact
save-exact=true
# Set the npm save
save=true
